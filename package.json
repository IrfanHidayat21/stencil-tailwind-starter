{
  "name": "tailwind-starter",
  "private": true,
  "version": "0.0.1",
  "description": "tailwind-starter",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "prerender": "stencil build --prerender",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "generate": "stencil generate",
    "buildCSS": "postcss src/public/tailwind/tailwind.css -o src/global/app.css",
    "watchCSS": "postcss src/public/tailwind/tailwind.css -o src/global/app.css --watch"
  },
  "devDependencies": {
    "@ionic/core": "^5.2.0",
    "@stencil/core": "^1.14.0",
    "@stencil/router": "^1.0.1",
    "@stencil/state-tunnel": "^1.0.1",
    "@types/node": "^14.0.13",
    "autoprefixer": "^9.8.0",
    "node-fetch": "^2.6.0",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.5",
    "workbox-build": "4.3.1"
  }
}
